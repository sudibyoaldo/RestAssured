<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="RestAssured_TheMovieDB" verbose="1">
    <listeners>
        <listener class-name="com.juaracoding.Listeners"></listener>
    </listeners>
    <parameter name="URL" value="https://www.themoviedb.org/"></parameter>
    <test name="Chrome">
        <parameter name="Browser" value="chrome"></parameter>
        <classes>
            <class name="com.juaracoding.TheMovieDB.TheMovieDB"/>
        </classes>
    </test>
</suite>